package baitap2.yeucau2;

public class Sum {
    private int sum = 0;
    private String aaa = "ok";
    
    public Sum() {
    }
    
    public void caculator(int[] arr) {
	int sumArr = 0;
	    for (int i = 0; i < arr.length; i++) {
		sumArr += arr[i];
		System.out.println("t "+arr[i]);
		}
	    synchronized (this.aaa) {
		    this.sum += sumArr;
		        System.out.println("S = " + this.sum);
		}

	    for (int i = 0; i < arr.length; i++) {
		System.out.println("d "+arr[i]);
		}
    }

}
