package fa.training.main;

import java.io.FileWriter;
import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.util.Properties;

import fa.training.util.DBUtils;
import fa.training.util.SQLCommand;

public class SaleManagement {

public static void main(String[] args) {
    try {
	Connection connection = DBUtils.getInstance().getConnection();
	PreparedStatement preparedStatement = connection.prepareStatement(SQLCommand.SELECT_ALL_CUSTOMER);
	ResultSet resultSet = preparedStatement.executeQuery();
	while (resultSet.next()) {
	    System.out.println(resultSet.getString("customer_name"));
	}
    } catch (Exception e) {
	e.printStackTrace();
    }
}
}
