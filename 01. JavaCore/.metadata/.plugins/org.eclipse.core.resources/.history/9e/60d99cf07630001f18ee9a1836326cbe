package baitap2.yeucau2;

public class Sum {
    private int sum = 0;

    public Sum() {
    }

    public int getSum() {
	return sum;
    }

    public void caculator(int[] arr) {
	this.sum = 0;
	for (int i = 0; i < arr.length; i++) {
	    this.sum += arr[i];
	}
	System.out.println("kq " + this.sum);
    }

}
