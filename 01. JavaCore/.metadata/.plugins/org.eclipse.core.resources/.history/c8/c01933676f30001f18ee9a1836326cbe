package baitap2.yeucau2;

public class Sum {
    private int sum = 0;
    
    public Sum() {
    }
    
    public void caculator(int[] arr) {
	synchronized (sum) {
	    this.sum += sum;
	        System.out.println("S = " + this.sum);
	}
	int sumArr = 0;
	    for (int i = 0; i < arr.length; i++) {
		sumArr += arr[i];
		}
    }

}
