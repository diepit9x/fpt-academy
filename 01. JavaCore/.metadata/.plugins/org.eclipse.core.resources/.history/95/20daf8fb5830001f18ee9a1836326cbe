package baitap1.cach3;

public class Shape {
    private String color;
    private String type;
    
    public Shape(String color) {
	this.color = color;
	this.type = "Shape";
    }

    public String getColor() {
        return color;
    }

    public void setColor(String color) {
        this.color = color;
    }
    
    public String draw() {
	return "I'm a " + this.color + " " + getClass().getSimpleName();
    }
}

