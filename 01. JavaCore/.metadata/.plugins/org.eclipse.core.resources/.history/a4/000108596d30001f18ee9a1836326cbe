package baitap2.yeucau2;

public class Test {
    public static void main(String[] args) {
	CreateArray createArray = new CreateArray();
	//sum = 55;
	
	
	Runnable task1 = new ArrayTask(createArray);
	Runnable task2 = new ArrayTask(createArray);
	
	Thread thread1 = new Thread(task1);
	Thread thread2 = new Thread(task2);
	
	thread1.start();
	thread2.start();
	
	try {
	    thread1.join();
	    thread2.join();
	} catch (Exception e) {
	    e.printStackTrace();
	}

	System.out.println("sum: " + createArray.getSum());
	
	
    }

}
