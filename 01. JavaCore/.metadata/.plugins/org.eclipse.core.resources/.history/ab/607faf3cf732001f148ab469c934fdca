package fa.training.service;

import java.sql.SQLException;
import java.sql.Date;

import fa.training.dao.CandidateDAO;
import fa.training.dao.CandidateDAOImpl;
import fa.training.entities.Candidate;
import fa.training.entities.Experience;

public class CandidateService {

public static void main(String[] args) {
    CandidateDAO candidateDAO = new CandidateDAOImpl();
    
   Candidate c1 = new Experience(1, "fresh1", Date.valueOf("2003-10-14"), "0987654321", "email@123", 1, 2, "skill");
   try {
    candidateDAO.addCandidate(c1);
} catch (SQLException e) {
    // TODO Auto-generated catch block
    e.printStackTrace();
} catch (Exception e) {
    // TODO Auto-generated catch block
    e.printStackTrace();
}
    
}

}
